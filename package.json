		"@sveltejs/adapter-static": "^2.0.2",
		"@sveltejs/kit": "^1.18.0",
		"@testing-library/svelte": "^3.2.2",
		"@types/marked": "^4.3.0",
		"@typescript-eslint/eslint-plugin": "^5.60.1",
		"@typescript-eslint/parser": "^5.59.2",
		"autoprefixer": "^10.4.14",
		"contentful": "^10.3.5",
		"dayjs": "^1.11.9",
		"eslint": "^8.43.0",
		"eslint-config-prettier": "^8.5.0",
