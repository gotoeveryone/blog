		"svelte-check": "^3.4.4",
		"svelte-fa": "^3.0.4",
		"svelte-meta-tags": "^2.8.0",
		"svelte-sitemap": "^2.6.0",
		"tailwindcss": "^3.3.2",
		"tslib": "^2.6.0",
		"typescript": "^5.1.6",
		"vite": "^4.4.6",
		"vitest": "^0.32.2"
